{
  "name": "@withyou/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npm run build --workspace @withyou/shared && prisma generate && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "prisma": "prisma",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:seed": "tsx prisma/seed.ts",
    "seed:dev": "tsx prisma/seed-dev.ts",
    "seed:e2e": "tsx prisma/seed-e2e.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "prisma": "^6.19.1",
    "tsx": "^4.21.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.967.0",
    "@aws-sdk/client-ses": "^3.967.0",
    "@aws-sdk/s3-presigned-post": "^3.967.0",
    "@aws-sdk/s3-request-presigner": "^3.967.0",
    "@prisma/client": "^6.4.1",
    "@withyou/shared": "file:../../packages/shared",
    "apple-signin-auth": "^2.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "zod": "^4.3.4"
  }
}
